<div class="well">
  <h2>Edit User Account</h2><hr/>
  <%= form_for @user, url: admin_user_path(@user.id) do |f| %>
    <div class="form-group">
      <h3><%= f.text_field :name %></h3>
    </div><br><br/>
    <div class="form-inline">
      <div class="form-group">
        <%= f.label(:email, "Email") %>
        <%= f.text_field :email%>
      </div>
      <div class="form-group">
        <%= f.label(:title, "Title") %>
        <%= f.text_field :title %>
      </div>
    </div><br/>
    <div class="text-right">
      <%= f.submit "Submit", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
